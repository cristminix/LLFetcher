{
	"name" : "LLFetcher",
	"version" : "1.0",
	"manifest_version" : 3,
	"permissions":[
		"scripting","storage",
        "webNavigation","tabs","cookies",
    "nativeMessaging"
	],
	"content_scripts" : [
		{
			
			"matches":["http://*/*", "https://*/*"],
			"js":["inject.js","function.js","content.js"]
		}
	],
	"background" : {
		"service_worker" : "background.js"
	},
	"web_accessible_resources": [
	   {
	     "resources": ["injected.js","function.js","assets/bootstrap-3.4.1-dist/css/bootstrap.min.css","assets/bootstrap-3.4.1-dist/css/bootstrap.theme.min.css","assets/font-awesome-4.6.3/font-awesome.min.css"],
	     "matches": ["http://*/*", "https://*/*"]
	   }
	 ],
	"host_permissions":[
		"https://www.linkedin.com/learning/*"
	],
	"description" : "LinkedIn Learning Fetcher",
	"action" : {
		"default_title" : "LinkedIn Learning Fetcher",
		"default_popup" : "index.html",
		"default_icon": {
	      "32": "ll_icon_32.png"
	    }
	}
	
}