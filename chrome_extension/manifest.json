{
	"name" : "LLFetcher",
	"version" : "1.0",
	"manifest_version" : 3,
	"permissions":[
		"scripting","storage",
        "webNavigation","tabs","cookies",
    	"downloads"
	],
	"content_scripts" : [
		{
			
			"matches":["https://www.linkedin.com/*"],
			"js":["assets/js/lib/underscore-min.js","inject.js","function.js","content.js"]
		}
	],
	"background" : {
		"service_worker" : "background.js"
	},
	"web_accessible_resources": [
	   {
	     "resources": ["injected.js","function.js"],
	     "matches": ["http://*/*", "https://*/*"]
	   }
	 ],
	"host_permissions":[
		"https://www.linkedin.com/learning/*"
	],
	"description" : "LinkedIn Learning Fetcher",
	"action" : {
		"default_title" : "LinkedIn Learning Fetcher",
		"default_popup" : "index.html",
		"default_icon": {
	      "32": "ll_icon_32.png",
	      "48": "ll_icon_32.png",
	      "128": "ll_icon_32.png"
	    }
	}
	
}