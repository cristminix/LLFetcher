{
  "manifest_version": 3,
  "name": "LLFetcher",
  "version": "2.0.0",
  "description": "LLFetcher is stand for Linked Learning Fetcher, which just simple chrome extension that can help you download all resource in Linked learning Course",
  "permissions": [
    "scripting",
    "storage",
    "webNavigation",
    "tabs",
    "cookies",
    "downloads"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.content.js.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://www.linkedin.com/learning/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ],
      "use_dynamic_url": true
    }
  ]
}