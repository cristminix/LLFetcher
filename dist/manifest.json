{
  "manifest_version": 3,
  "name": "LLFetcher",
  "version": "2.0.0",
  "description": "LLFetcher is stand for Linked Learning Fetcher, which just simple chrome extension that can help you download all resource in Linked learning Course",
  "permissions": [
    "scripting",
    "storage",
    "webNavigation",
    "tabs",
    "cookies",
    "downloads",
    "nativeMessaging"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.content.js.63797b2b.50aee84a.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "host_permissions": [
    "https://www.linkedin.com/learning/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "19": "images/icon-19.png",
      "32": "images/icon-32.png",
      "38": "images/icon-38.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "resources": [
        "assets/content.js.63797b2b.js",
        "lib/*"
      ],
      "use_dynamic_url": true
    }
  ]
}