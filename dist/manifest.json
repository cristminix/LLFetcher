{
  "manifest_version": 3,
  "name": "LLFetcher",
  "version": "3.0.0",
  "description": "Download LinkedIn Learning videos or media files, subtitle, playlist and course assets easily !",
  "permissions": [
    "scripting",
    "storage",
    "webNavigation",
    "tabs",
    "cookies",
    "downloads",
    "nativeMessaging"
  ],
  "host_permissions": [
    "https://www.linkedin.com/learning/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.jsx-loader-0YldQdSZ.js"
      ],
      "matches": [
        "https://www.linkedin.com/learning/*"
      ],
      "css": [
        "assets/fn-6PmZOoQv.css"
      ]
    }
  ],
  "options_ui": {
    "page": "src/option-pages/options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "logo/icon-16.png",
    "19": "logo/icon-19.png",
    "32": "logo/icon-32.png",
    "38": "logo/icon-38.png",
    "48": "logo/icon-48.png",
    "128": "logo/icon-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo/icon-16.png",
      "19": "logo/icon-19.png",
      "32": "logo/icon-32.png",
      "38": "logo/icon-38.png",
      "48": "logo/icon-48.png",
      "128": "logo/icon-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "resources": [
        "assets/fn-tlBbayZr.js",
        "assets/jquery-etZba0_3.js",
        "assets/content.jsx-xtvy4qnW.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
