{
  "name": "vue3-esbuild",
  "version": "1.0.0",
  "main": "lib/{{name}}.js",
  "module": "lib/{{name}}.js",
  "files": [
    "lib",
    "types",
    "packages",
    "LLFetcher"
  ],
  "types": "types/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "cristminix",
  "description": "",
  "scripts": {
    "serve": "npm run clean && node esbuild.config.js serve",
    "build:popup": "node esbuild.config.popup.js",
    "clean": "rimraf ./lib ./examples/static ./LLFetcher/chrome_extension/popup.js",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore .",
    "dts": "vue-tsc --declaration --emitDeclarationOnly",
    "dts:check": "vue-tsc --noEmit",
    "build:background": "esbuild --bundle ./LLFetcher/src/service_workers/background.ts --outdir=./LLFetcher/chrome_extension --sourcemap --minify",
    "build:content": "esbuild --bundle ./LLFetcher/src/content_scripts/content.ts --outdir=./LLFetcher/chrome_extension --sourcemap --minify&&esbuild --bundle ./LLFetcher/src/content_scripts/function.ts --outdir=./LLFetcher/chrome_extension --sourcemap --minify&&esbuild --bundle ./LLFetcher/src/content_scripts/inject.ts --outdir=./LLFetcher/chrome_extension --sourcemap --minify"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.202",
    "@types/jquery": "^3.5.14",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "@vue/compiler-sfc": "^3.2.45",
    "autoprefixer": "^10.4.13",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.3",
    "browser-sync": "^2.27.10",
    "esbuild": "^0.14.54",
    "esbuild-plugin-browser-sync": "^0.1.1",
    "esbuild-plugin-filesize": "^0.3.0",
    "esbuild-plugin-progress": "^1.0.1",
    "esbuild-plugin-vue": "^0.2.4",
    "esbuild-sass-plugin": "^2.4.3",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "fontawesome-4.7": "^4.7.0",
    "jquery": "^3.6.1",
    "localstoragedb": "^2.3.2",
    "postcss": "^8.4.19",
    "postcss-import": "^15.1.0",
    "postcss-minify": "^1.1.0",
    "postcss-preset-env": "^7.8.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "typescript": "^4.9.4",
    "underscore": "^1.13.6",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11"
  },
  "pre-commit": [
    "lint"
  ]
}
